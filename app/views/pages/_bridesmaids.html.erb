<div class = "container">
  <div class="row">
    <h2>Bridesmaids</h2>
    <div class="hr"></div>
    <% @bridesmaids.each do |b| %>
      <div class="col-lg-3 col-lg-1">
        <div class="people-item">
          <% @photo = b["photo"] %>
          <%= image_tag(@photo, class: "img-responsive") %>
          <div class="description">
            <h3><%=b["name"]%></h3>
          </div>
        </div>
      </div>
    <% end%>
  </div> 
</div>